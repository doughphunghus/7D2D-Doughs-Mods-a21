<!-- DOUGH: MUST use CRLF line endings on any CML file using csv! else emsses things up! -->

<!-- NORMAL -->
<csv xpath="/entitygroups/entitygroup[contains(@name,'feralHordeStageGS') and contains(text(), 'zombieSpider' ) ]/text()" delim="\n" op="add" >
